{
  "name": "hodl-totals",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile": "tsc && for file in $(find dist -name '*.js' -o -name '*.map'); do NEWFILENAME=`echo \"$file\" | sed 's/.js$/.mjs/g; s/.js.map$/.mjs.map/g'`; mv \"$file\" $NEWFILENAME; done",
    "compile-watch": "tsc-watch --onsuccess \"node dist/index.js\"",
    "lint": "eslint -c .eslintrc.js .",
    "test:e2e": "echo \"Error: no e2e test specified, i.e. run with cmd node e2e \" && exit 1",
    "test:unit": "jest",
    "test:unit-watch": "jest --watchAll",
    "test": "npm run test:unit && npm run test:e2e",
    "deploy": "clasp push",
    "build": "npm run lint && npm run compile",
    "start": "npm run build && node --experimental-specifier-resolution=node dist/index.mjs",
    "debug": "npm run compile && node --experimental-specifier-resolution=node --inspect dist/index.mjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@ni/eslint-config": "*",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "eslint": "^7.21.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "tsc-watch": "^2.1.2",
    "typescript": "^4.2.4"
  }
}
